while (prov.toString().indexOf(&quot;${KTR}&quot;) != -1)
	prov=prov.replace(&quot;${KTR}&quot;,ktr);

while (prov.toString().indexOf(&quot;${TODAY}&quot;) != -1)
	prov=prov.replace(&quot;${TODAY}&quot;,today);
